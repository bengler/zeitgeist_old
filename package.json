{
  "name": "zeitgeist",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch-test": "npm run test -- --watch",
    "test": "mocha --compilers js:babel-core/register test/**/*.js",
    "start": "remon --require ./.init.js bin/www",
    "lint": "eslint --ext .js .",
    "pre-commit-check": "(npm run lint -- --quiet || (echo 'Warning: project has lint errors. Please fix and re-commit with `git commit --amend`' && echo ''))"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel": "^5.8.23",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.3",
    "babel-preset-es2015": "^6.1.18",
    "chai": "^3.4.1",
    "eslint": "^1.6.0",
    "eslint-config-bengler": "^0.3.3",
    "eslint-plugin-import": "^0.7.9",
    "mocha": "^2.3.4",
    "remon": "^1.0.2",
    "supertest": "^1.1.0"
  }
}
